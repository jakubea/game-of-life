{
  "private": true,
  "engines": {
    "yarn": ">=1.12.1"
  },
  "scripts": {
    "build": "run-s build:app",
    "build:app": "node scripts/build.js",
    "dev": "run-s dev:start",
    "dev:nodebug": "cross-env ELM_DEBUGGER=false run-s dev",
    "dev:start": "cross-env BROWSER=none node scripts/start.js",
    "elm": "elm",
    "elm:analyse": "elm-analyse --elm-format-path ./node_modules/.bin/elm-format",
    "elm:analyse:serve": "run-s \"elm:analyse --serve --port 3030\"",
    "elm:install": "node --version",
    "elm:make": "elm make",
    "elm:outdated": "elm-outdated",
    "elm:reactor": "elm reactor",
    "elm:repl": "elm repl",
    "format": "run-s format:elm-format format:prettier",
    "format:elm-format": "elm-format --yes ./src/ ./tests/",
    "format:prettier": "prettier --write \"bin/**/*.js\" \"src/**/*.js\" \"src/**/*.json\" README.md elm-analyse.json",
    "i18n:missing-translations": "node ./bin/i18n/missing-translations.js",
    "i18n:export-translations": "node ./bin/i18n/export-translations.js",
    "i18n:import-translations": "node ./bin/i18n/import-translations.js",
    "precommit": "run-s --print-name format i18n:missing-translations test",
    "test": "elm-test",
    "test:coverage": "elm-coverage --elm-test ./node_modules/.bin/elm-test",
    "test:watch": "run-s \"test --watch\"",
    "svgo": "svgo -f ./public/images/svg-assets/"
  },
  "dependencies": {
    "browser-cookies": "1.2.0",
    "css-element-queries": "1.1.1",
    "jwt-decode": "^2.2.0",
    "memorystorage": "^0.11.0",
    "scroll-to-element": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/plugin-transform-runtime": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.4.9",
    "babel-loader": "8.0.5",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "2.4.2",
    "clean-webpack-plugin": "^1.0.1",
    "connect-history-api-fallback": "^1.6.0",
    "cosmiconfig": "^5.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.11",
    "dotenv": "^6.2.0",
    "elm": "^0.19.0-bugfix6",
    "elm-analyse": "^0.16.1",
    "elm-coverage": "0.2.0",
    "elm-format": "^0.8.1",
    "elm-hot-webpack-loader": "^1.0.0",
    "elm-outdated": "1.1.0",
    "elm-test": "0.19.0-rev5",
    "elm-webpack-loader": "^5.0.0",
    "fast-csv": "^2.4.1",
    "fast-json-stable-stringify": "^2.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "http-proxy-middleware": "^0.19.1",
    "mini-css-extract-plugin": "^0.5.0",
    "minimist": "1.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "3.0.0",
    "prettier": "^1.16.4",
    "promise": "8.0.2",
    "ramda": "0.26.1",
    "react-dev-utils": "7.0.3",
    "react-error-overlay": "^5.1.3",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.2.1",
    "webpack-manifest-plugin": "^2.0.3",
    "whatwg-fetch": "3.0.0"
  }
}
